{
  "name": "restfulrant-server",
  "version": "1.0.0",
  "description": "api layer for restfulrant",
  "main": "dist/index.js",
  "repository": {
    "type": "ssh",
    "url": "git@github.com:AndrewThian/server.git"
  },
  "scripts": {
    "build": "tsc",
    "prettify:ts": "prettier --write --config ./.prettierrc '{src,tests}/**/*.ts'",
    "lint:ts": "tslint -c tslint.json '{src,tests}/**/*.ts'",
    "start": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "test-server": "NODE_ENV=test ts-node src/index.ts",
    "test": "NODE_ENV=test jest || true",
    "test:unit": "NODE_ENV=test jest ./src/tests/unit/ || true",
    "test:integration": "NODE_ENV=test jest ./src/tests/integration/ || true",
    "seed:restaurant": "NODE_ENV=test ts-node -r tsconfig-paths/register src/parse.ts"
  },
  "keywords": [
    "api",
    "restuarant",
    "todo"
  ],
  "author": "Andrew Thian",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "errorhandler": "^1.5.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@types/csv-parse": "^1.1.11",
    "@types/express": "^4.16.0",
    "csv-parse": "^4.1.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "reflect-metadata": "^0.1.12",
    "stream-transform": "^1.0.7",
    "typeorm": "^0.2.9",
    "typeorm-model-generator": "^0.2.22"
  }
}

{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [false],
        "no-console": [false],
        "quotemark": [true, "double"],
        "object-literal-sort-keys": [false],
        "ordered-imports": [false],
        "no-var-requires": true,
        "member-access": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    }
}